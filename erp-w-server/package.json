{
  "name": "user-service",
  "version": "1.0",
  "description": "mini erp welyne",
  "main": "dist/server.js",
  "author": "Oumayma benkhalifa @Welyne",
  "scripts": {
    "run": "nodemon dist/server.js",
    "start": "yarn run build && env-cmd -f .env node dist/server.js",
    "debug": "yarn run build && yarn run watch",
    "debug:dev": "yarn run build && yarn run watch:debug",
    "build": "yarn run build:ts && yarn run lint",
    "build:ts": "tsc --rootDir ./src",
    "serve": "env-cmd -f .env nodemon --on-change-only -q -w dist dist/server.js",
    "serve:debug": "env-cmd -f .env.dev nodemon --on-change-only -q -w dist dist/server.js",
    "watch": "concurrently --kill-others --names \"BUILD,RUN\" -c \"yellow.bold,cyan.bold\" \"yarn run watch:ts\" \"yarn run serve\"",
    "watch:debug": "concurrently --kill-others --names \"BUILD,RUN\" -c \"yellow.bold,cyan.bold\" \"yarn run watch:ts\" \"yarn run serve:debug\"",
    "watch:ts": "tsc -w",
    "lint": "eslint --fix src/**/*.ts",
    "clean": "rm -rf node_modules && rm -f yarn.lock && yarn install",
    "test": "env-cmd -f .env.test jest --watchAll --no-cache --coverage",
    "test:ci": "env-cmd -f .env.test jest --collectCoverage --coverageDirectory=\"./coverage\" --ci --reporters=default --reporters=jest-junit --watchAll=false"
  },
  "dependencies": {
    "@aws-sdk/client-rekognition": "^3.95.0",
    "@aws-sdk/client-s3": "^3.99.0",
    "@aws-sdk/client-textract": "^3.99.0",
    "@hapi/joi": "^17.1.1",
    "@types/jwt-decode": "^3.1.0",
    "bcryptjs": "^2.4.3",
    "bluebird": "^3.7.2",
    "body-parser": "^1.20.0",
    "config": "^3.3.7",
    "dotenv": "^16.0.0",
    "env-cmd": "^10.1.0",
    "express": "^4.17.3",
    "express-session": "^1.17.3",
    "express-validator": "^6.14.2",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^3.1.2",
    "module-alias": "^2.2.2",
    "moment": "^2.29.3",
    "momentjs": "^2.0.0",
    "mongoose": "^6.3.8",
    "mongoose-enumvalues": "^0.0.3",
    "mongoose-schema-extend": "^0.2.2",
    "multer": "^1.4.4",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "passport-oauth2": "^1.6.1",
    "winston": "^3.7.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/bluebird": "^3.5.36",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.4",
    "@types/hapi__joi": "^17.1.8",
    "@types/jest": "^27.4.1",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.0.0",
    "@types/passport": "^1.0.7",
    "@types/passport-jwt": "^3.0.6",
    "@types/passport-oauth2": "^1.4.11",
    "@types/sinon": "^10.0.11",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "concurrently": "^7.2.1",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.5.1",
    "jest-junit": "^13.2.0",
    "jest-watch-typeahead": "^1.0.0",
    "mongodb-memory-server": "^8.4.1",
    "nodemon": "^2.0.16",
    "passport-mock-strategy": "^2.0.0",
    "prettier": "^2.6.1",
    "sinon": "^14.0.0",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.3"
  },
  "_moduleAliases": {
    "@errors": "dist/errors",
    "@logger": "dist/logger",
    "@middlewares": "dist/middlewares",
    "@models": "dist/models"
  }
}
